{
  "parameters": {
    "N8N_API_URL": {
      "description": "n8n Cloud API URL",
      "example": "https://qtalospace.app.n8n.cloud/api/v1"
    },
    "N8N_API_KEY": {
      "description": "n8n API Key (JWT token)",
      "sensitive": true
    },
    "N8N_HOST": {
      "description": "n8n Cloud hostname",
      "example": "qtalospace.app.n8n.cloud"
    },
    "N8N_PROJECT_ID": {
      "description": "QTalo project ID in n8n",
      "example": "BFIDAvXOISVZh7tb"
    },
    "AWS_API_GATEWAY_URL": {
      "description": "AWS API Gateway base URL for client management",
      "example": "https://r81lwr2etg.execute-api.us-east-1.amazonaws.com/prod"
    },
    "AWS_API_KEY": {
      "description": "AWS API Gateway API Key",
      "sensitive": true
    },
    "CLICKUP_API_URL": {
      "description": "ClickUp API base URL",
      "example": "https://api.clickup.com/api/v2"
    },
    "GITHUB_OWNER": {
      "description": "GitHub repository owner",
      "example": "byronmartinez-ballastlane"
    },
    "GITHUB_REPO": {
      "description": "GitHub repository name",
      "example": "qtalo-n8n-testing"
    },
    "GITHUB_CREDENTIAL_ID": {
      "description": "n8n GitHub credential ID",
      "example": "Q6oSPWOEORCm73TJ"
    },
    "N8N_CREDENTIAL_ID": {
      "description": "n8n API credential ID (for self-referencing)",
      "example": "EaOQcyWDYyvGbbr7"
    },
    "CLICKUP_SYSTEM_CREDENTIAL_ID": {
      "description": "n8n ClickUp credential ID for system workflows",
      "example": "KQ4DCkxv3kBoRgK1"
    }
  },
  "workflows": {
    "client-onboarding-v2": {
      "source": "system/client-onboarding-v2.template.json",
      "output": "system/client-onboarding-v2.json",
      "description": "Client onboarding workflow - creates workflows from templates"
    },
    "status-change-router": {
      "source": "system/status-change-router.template.json",
      "output": "system/status-change-router.json",
      "description": "Routes ClickUp status changes to trigger appropriate phases"
    },
    "system-jwt-rotation": {
      "source": "system/system-jwt-rotation.template.json",
      "output": "system/system-jwt-rotation.json",
      "description": "Rotates JWT tokens for n8n API access"
    }
  }
}
